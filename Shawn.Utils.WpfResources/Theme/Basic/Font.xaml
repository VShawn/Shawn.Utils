<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:Double x:Key="DefaultControlHeight">30</system:Double>
    <Thickness x:Key="DefaultControlPadding">10,6</Thickness>
    <system:Double x:Key="DefaultCornerRadiusValue">4</system:Double>
    <CornerRadius x:Key="DefaultCornerRadius" TopLeft="{StaticResource DefaultCornerRadiusValue}" TopRight="{StaticResource DefaultCornerRadiusValue}" BottomLeft="{StaticResource DefaultCornerRadiusValue}" BottomRight="{StaticResource DefaultCornerRadiusValue}"></CornerRadius>
</ResourceDictionary>